<?xml version="1.0" encoding="UTF-8"?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="OMShopMobile.CategoryListCellXaml">
	<ViewCell.View>
		<StackLayout Padding="16,0" Orientation="Horizontal">
			<Label x:Name="lblName" VerticalTextAlignment="Center" Text="{Binding Description.Name}" />
			<Image HorizontalOptions="EndAndExpand" Source="Catalog/arrow_catalog_.png" />
		</StackLayout>
	</ViewCell.View>
</ViewCell>
